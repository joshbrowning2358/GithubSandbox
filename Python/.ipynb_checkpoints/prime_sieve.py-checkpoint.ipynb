    "def getPrimes(N_max):\n",
    "    is_prime = np.ones(N_max, dtype = bool)\n",
    "    max_value = np.sqrt(N_max)\n",
    "    for i in np.arange(2, max_value):\n",
    "        is_prime[i**2::i] = False\n",
    "    return(np.arange(N_max)[is_prime])"
   ]
